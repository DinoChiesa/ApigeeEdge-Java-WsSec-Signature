<AssignMessage name='AM-GoodResponse'>
  <Remove>
    <Headers/>
  </Remove>
  <Set>
    <Headers>
      <Header name='content-type'>application/xml</Header>
    </Headers>
    <StatusCode>200</StatusCode>
    <ReasonPhrase>OK</ReasonPhrase>
  </Set>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</AssignMessage>
