<JavaCallout name='Java-VerifySignature-KVMJKS'>
  <Properties>
    <Property name='alias'>{private.jksalias}</Property>
    <Property name='password'>{private.jkspassword}</Property>
    <Property name='jks-base64'>{private.jksbase64}</Property>
  </Properties>
  <ClassName>com.google.apigee.callout.wssec.SOAPVerifier</ClassName>
  <ResourceURL>java://edge-wssec-sign-x509-1.0.3.jar</ResourceURL>
</JavaCallout>
